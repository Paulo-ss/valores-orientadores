{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ],
  "crons": [
    {
      "path": "/vi/update-cas-vi-file-job",
      "schedule": "0 2 * 5,11 *"
    },
    {
      "path": "/vi/test-cron-job",
      "schedule": "0 13 * * *"
    }
  ]
}
